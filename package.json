{
  "name": "@telicent-oss/telicent-frontend-cli",
  "type": "module",
  "version": "0.0.1-dev3.1",
  "scripts": {
    "build": "rm -rf ./dist && tsc && chmod +x ./dist/bin/index.js",
    "build:watch": "chokidar 'src/**/*.ts' -c 'tsc'",
    "prepare": "yarn build && husky",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "readme": "node ./scripts/updateReadmeWithTefeHelpCommand.js",
    "relink": "(yarn unlink || ./scripts/forceUnlink.cjs) && yarn link"
  },
  "bin": {
    "tefe": "./dist/bin/index.js"
  },
  "description": "A CLI for FE engineers",
  "main": "./src/index.js",
  "keywords": [
    "cli",
    "telicent"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@commander-js/extra-typings": "12.0.1",
    "@types/update-notifier": "^6.0.8",
    "commander": "12.0.0",
    "update-notifier": "6.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "1.4.0",
    "chokidar-cli": "3.0.0",
    "husky": "9.0.11",
    "typescript": "^5.4.5",
    "vitest": "1.4.0"
  }
}
