{
  "name": "@telicent-oss/telicent-frontend-cli",
  "type": "module",
  "version": "1.0.1-dev.4",
  "scripts": {
    "build": "rm -rf ./dist && tsc && chmod +x ./dist/bin/tefe.js",
    "build:watch": "chokidar 'src/**/*.ts' -c 'yarn build'",
    "prepare": "yarn build && husky",
    "test": "vitest --watch=false",
    "test:watch": "vitest",
    "lint": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "readme": "node ./scripts/updateReadmeWithTefeHelpCommand.js",
    "relink": "(yarn unlink || ./scripts/forceUnlink.cjs) && yarn link",
    "push": "git push && git push --tags",
    "publish:prerelease": "yarn version --prerelease && yarn push",
    "publish:p": "yarn publish:prerelease"
  },
  "bin": {
    "tefe": "./dist/bin/tefe.js"
  },
  "description": "A CLI for FE engineers",
  "main": "./src/index.js",
  "keywords": [
    "cli",
    "telicent"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/telicent-oss/telicent-frontend-cli"
  },
  "dependencies": {
    "@commander-js/extra-typings": "12.0.1",
    "@types/update-notifier": "^6.0.8",
    "commander": "12.0.0",
    "memfs": "4.9.2",
    "update-notifier": "6.0.2",
    "yaml": "2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/coverage-v8": "1.4.0",
    "chokidar-cli": "3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "9.0.11",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "1.4.0"
  },
  "engine": {
    "node": "20.8.1"
  }
}
